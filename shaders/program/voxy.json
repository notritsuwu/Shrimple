{
  "unusedString": "
    #include "/lib/constants.glsl"
    #include "/lib/common.glsl"
  ",
  "version": 1,
  "excludeLodsFromVanillaDepth": false,
  #if defined(LIGHTING_REFLECT_ENABLED) || defined(PHOTONICS_LIGHT_ENABLED)
    #ifdef PHOTONICS_LIGHT_ENABLED
      "opaqueDrawBuffers": [0,1,2,3],
      "translucentDrawBuffers": [0,1,2,3],
    #else
      "opaqueDrawBuffers": [0,1,2],
      "translucentDrawBuffers": [0,1,2],
    #endif
  #else
    "opaqueDrawBuffers": [0],
    "translucentDrawBuffers": [0],
  #endif
  #ifdef TAA_ENABLED
    "taaOffset": "{return taa_offset;}",
  #endif
  "uniforms": [
    "cameraPosition",
    "cloudHeight",
    "cloudTime",
    "eyePosition",
    "fogColor",
    "fogEnd",
    "fogStart",
    "gbufferModelView",
    "gbufferModelViewInverse",
    "isEyeInWater",
    "rainStrength",
    "shadowLightPosition",
    "skyColor",
    "sunLocalDir",
    "taa_offset",
    "viewSize",
    "vxModelViewInv",
    "vxProjInv",
    "vxRenderDistance"
  ],
  "samplers": {
    #ifdef SHADOW_CLOUDS
      "texCloudShadow": "sampler2D",
    #endif
    "lightmap": "sampler2D"
  }
}
